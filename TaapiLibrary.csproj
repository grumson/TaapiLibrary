<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PackageId>Grumson.Utilities.Taapi</PackageId>
		<Version>2.0.3</Version>
		<FileVersion>2.0.3</FileVersion>
		<Authors>Dejan Grum</Authors>
		<Company>Grumson</Company>
		<Description>Taapi library</Description>
		<PackageTags>taapi, crypto, trading, bot, signals, indicators</PackageTags>
		<!-- Dodajte URL vašega projekta tukaj -->
		<PackageProjectUrl>https://github.com/grumson/TaapiLibrary.git</PackageProjectUrl>
		<!-- Dodajte URL vašega repozitorija tukaj -->
		<RepositoryUrl>https://github.com/grumson/TaapiLibrary.git</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<!-- Dodajte pot do LICENSE.txt -->
		<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
		<!-- Dodajte pot do README.md -->
		<PackageReadmeFile>README.md</PackageReadmeFile>
	</PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="9.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

	<ItemGroup>
		<None Include="LICENSE.txt" Pack="true" PackagePath="" />
		<None Include="README.md" Pack="true" PackagePath="" />
	</ItemGroup>


</Project>
