
// MULTY SYMBOL BULK INDICATORS REQUEST
// This example demonstrates how to request multiple symbols with multiple indicators in a single request.

{
  "secret": "MY_SECRET",
  "construct": [
    {
      "exchange": "binance",
      "symbol": "BTC/USDT",
      "interval": "1h",
      "type": "crypto",
      "indicators": [
        {
          "id": "BTC/USDT_rsi",
          "indicator": "rsi"
        },
        {
          "id": "BTC/USDT_cmf",
          "indicator": "cmf",
          "period": 20
        },
        {
          "id": "BTC/USDT_macd",
          "indicator": "macd",
          "backtrack": 1,
          "chart": "candles",
          "gaps": "true"
          // other options look documentation
        }
      ]
    },
    {
      "exchange": "binance",
      "symbol": "ETH/USDT",
      "interval": "1h",
      "type": "crypto",
      "indicators": [
        {
          "id": "ETH/USDT_rsi",
          "indicator": "rsi"
        },
        {
          "id": "ETH/USDT_cmf",
          "indicator": "cmf",
          "period": 20
        },
        {
          "id": "ETH/USDT_macd",
          "indicator": "macd",
          "backtrack": 1,
          "chart": "candles",
          "gaps": "true"
          // other options look documentation
        }
      ]
    },
    {
      "exchange": "binance",
      "symbol": "XRP/USDT",
      "interval": "1h",
      "type": "crypto",
      "indicators": [
        {
          "id": "XRP/USDT_rsi",
          "indicator": "rsi"
        },
        {
          "id": "XRP/USDT_cmf",
          "indicator": "cmf",
          "period": 20
        },
        {
          "id": "XRP/USDT_macd",
          "indicator": "macd",
          "backtrack": 1,
          "chart": "candles",
          "gaps": "true"
          // other options look documentation
        }
      ]
    }
  ]
}
